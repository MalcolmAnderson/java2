<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="390.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AddModify_ContactsController">
   <children>
      <HBox prefWidth="400.0">
         <children>
            <HBox prefWidth="200.0">
               <children>
                  <Label fx:id="lblScreenIdentifier" text="Add Part">
                     <font>
                        <Font name="System Bold" size="24.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets left="10.0" top="20.0" />
         </padding>
      </HBox>
      <GridPane vgap="15.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="lblContactID" text="Contact ID">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="lblName" text="Name" GridPane.rowIndex="1">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="lblEmailAddress" text="Email Address" GridPane.rowIndex="2">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="txtName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="txtEmailAddress" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label fx:id="id" text="Label" GridPane.columnIndex="1" />
         </children>
         <padding>
            <Insets left="40.0" />
         </padding>
      </GridPane>
      <HBox alignment="CENTER_RIGHT" prefWidth="200.0" spacing="30.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onSaveAction" text="Save">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#onCancelAction" text="Cancel">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets right="40.0" top="20.0" />
         </padding>
      </HBox>
   </children>
</VBox>
