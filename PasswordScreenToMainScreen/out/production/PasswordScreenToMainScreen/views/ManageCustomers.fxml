<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<!-- fx:controller="controllers.ManageCustomers_Controller" -->

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ManageCustomers_Controller">
   <children>
      <VBox alignment="CENTER" prefHeight="401.0" prefWidth="601.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="36.0" prefWidth="500.0" text="This page is for adding adding, updating and deleting contacts" textAlignment="RIGHT">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding></Label>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="400.0" prefWidth="601.0">
               <children>
                  <TableView prefHeight="289.0" prefWidth="500.0">
                     <columns>
                        <TableColumn prefWidth="100.0" text="Customer Id" />
                        <TableColumn prefWidth="200.0" text="Name" />
                        <TableColumn prefWidth="75.0" text="Address" />
                        <TableColumn minWidth="50.0" prefWidth="200.0" text="Email" />
                     </columns>
                  </TableView>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="40.0" prefWidth="581.0" spacing="20.0">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <children>
                  <Button mnemonicParsing="false" text="Add Customer" />
                  <Button mnemonicParsing="false" onAction="#onClick_EditContact" text="Edit Customer" />
                  <Button mnemonicParsing="false" text="Delete Customer" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="40.0" prefWidth="561.0" spacing="20.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#onClick_Cancel" text="Return to Appointment Screen" />
               </children>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
