<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Reports_Controller">
   <children>
      <VBox prefHeight="600.0" prefWidth="1100.0">
         <children>
            <HBox alignment="CENTER" prefHeight="520.0" prefWidth="1100.0">
               <children>
                  <TextArea fx:id="taReport" prefHeight="550.0" prefWidth="900.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="40.0" prefWidth="1100.0" spacing="30.0">
               <children>
                  <Button fx:id="btnAppointmentTypes" mnemonicParsing="false" onAction="#onClick_AppointmentByTypeAndByMonth" text="Appointments by Type and by Month" />
                  <Button fx:id="btnAppointmentsByContact" mnemonicParsing="false" onAction="#onClick_AppointmentsByContact" text="Appointments by Contact" />
                  <Button fx:id="btnAppointmentsByCustomer" mnemonicParsing="false" onAction="#onClick_AppointmentsByCustomer" text="Appointments by Customer" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="40.0" prefWidth="1100.0">
               <children>
                  <Button fx:id="btnReturnToAppointmentScreen" mnemonicParsing="false" onAction="#onClick_btnReturnToAppointmentScreen" text="Return to Appointment Screen" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
